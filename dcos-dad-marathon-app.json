{
  "id": "/dcos_dad_prefix-dcos-dad-slave",
  "instances": 1,
  "container": {
    "type": "DOCKER",
      "volumes": [
          {
              "containerPath": "/sys/fs/cgroup",
              "hostPath": "/sys/fs/cgroup",
              "mode": "RW"
          },
         {
        "persistent": {
          "size": 10000
        },
        "mode": "RW",
        "containerPath": "docker-storage-dir"
      }
      ],
    "docker": {
      "image": "jeid64/dcos-dad",
      "privileged": true,
      "network": "USER",
      "parameters": [
        {"key": "tmpfs", "value": "/run:rw,exec,nosuid,size=2097152k"},
        {"key": "tmpfs", "value": "/tmp:rw,exec,nosuid,size=2097152k"}
      ],
      "portMappings": [
        {
          "containerPort": 5051,
          "hostPort": 0,
          "labels": {
            "VIP_3": "/dcos-dad-slave:5051"
          },
          "protocol": "tcp",
          "name": "mesos-slave"
        }
    ],
    "forcePullImage": true
    }
  },
  "mem": 2500,
  "cpus": 2,
  "uris": [
          "file:////home/core/.ssh/authorized_keys"
  ],
  "ipAddress": {
          "networkName": "dcos"
  }
}
